    "mkdirp": "^0.5.1",
    "morgan": "^1.9.1",
    "polished": "^3.0.3",
    "query-string": "^6.4.0",
    "react": "^16.8.4",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dom": "^16.8.4",
    "react-easy-emoji": "^1.2.0",
    "reflect-metadata": "^0.1.13",

{
  "name": "@react-finland/graphql-api",
  "version": "0.0.0",
  "private": true,
  "description": "React Finland GraphQL API",
  "main": "./build/server",
  "scripts": {
    "generate:all": "npm run generate:indices && npm run generate:reason-conf",
    "generate:indices": "node utils/generate-indices.js content --verbose",
    "generate:reason-conf": "node utils/generate-reason-conf.js",
    "generate:typeof": "node utils/generate-typeof.js",
    "build": "npm run generate:all && rimraf build && tsc",
    "lint": "tslint --config tslint.json content/**/*.ts server/**/*.ts --fix",
    "start": "tsc-watch --onSuccess \"node ./build/server\" --onFailure \"echo Beep! Compilation Failed\"",
    "heroku:start": "node ./build/server",
    "format": "prettier content/**/*.ts server/**/*.ts --write",
    "heroku-postbuild": "echo Skip build on Heroku"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/compression": "0.0.36",
    "@types/cors": "^2.8.4",
    "@types/csv-parse": "^1.1.12",
    "@types/dotenv": "^6.1.0",
    "@types/express": "^4.16.1",
    "@types/express-graphql": "^0.6.2",
    "@types/file-saver": "^2.0.0",
    "@types/fs-extra": "^5.0.5",
    "@types/helmet": "0.0.43",
    "@types/history": "^4.7.2",
    "@types/lodash": "^4.14.123",
    "@types/md5": "^2.1.33",
    "@types/morgan": "^1.7.35",
    "@types/node": "^11.11.3",
    "@types/query-string": "^6.3.0",
    "@types/react": "^16.8.8",
    "@types/react-copy-to-clipboard": "^4.2.6",
    "@types/react-dom": "^16.8.2",
    "@types/request": "^2.48.1",
    "@types/request-promise-native": "^1.0.15",
    "camelcase": "^5.2.0",
    "cheerio": "^1.0.0-rc.2",
    "csstype": "^2.6.3",
    "flatmap": "0.0.3",
    "glob": "^7.1.3",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "mri": "^1.1.4",
    "prettier": "^1.16.4",
    "tsc-watch": "^2.1.2",
    "tslint": "^5.14.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ReactFinland/graphql-api.git"
  },
  "bugs": {
    "url": "https://github.com/ReactFinland/graphql-api/issues"
  },
  "dependencies": {
    "@emotion/core": "^10.0.9",
    "@emotion/styled": "^10.0.9",
    "bent": "^1.4.0",
    "body-parser": "^1.18.3",
    "cloudinary": "^1.13.2",
    "compression": "^1.7.3",
    "cors": "^2.8.5",
    "csv-parse": "^4.3.4",
    "csv-stringify": "^5.3.0",
    "date-range-array": "^2.1.0",
    "dotenv": "^7.0.0",
    "expeditious-engine-memory": "^0.2.1",
    "express": "^4.16.4",
    "express-expeditious": "^5.0.0",
    "express-graphql": "^0.7.1",
    "express-http-to-https": "^1.1.4",
    "file-saver": "^2.0.1",
